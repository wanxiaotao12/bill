<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig     
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"     
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
    
<sqlMapConfig>
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="false" errorTracingEnabled="true" maxRequests="32"
		maxSessions="10" maxTransactions="5" useStatementNamespaces="true" />
 	<sqlMap resource="sqlmap/jrp.commons.xml" />
	<!-- privilege start-->
 	<sqlMap resource="sqlmap/DETAILBILL.xml" />
	<sqlMap resource="sqlmap/MINE.xml" />
	<sqlMap resource="sqlmap/PROFITBILL.xml" />
	<sqlMap resource="sqlmap/SHIPPERBILL.xml" />
	<sqlMap resource="sqlmap/CardDetail.xml" />
	<sqlMap resource="sqlmap/CardDailyStatistics.xml" />
	<sqlMap resource="sqlmap/Account.xml" />
	<sqlMap resource="sqlmap/Dictionary.xml" />

</sqlMapConfig>
