<!--mainContent-->
   <div class="mainContent fl">
    	<div class="crumb_fund">
        	<span>运营后台</span> &gt; <span>渠道管控中心</span> &gt; <span class="cur"></span>
        </div>
        <form action="../shipperbill/shipperbillIndex.do" method="post"  id="formSubmit">
        <div class="contentList borderNo">
        	<div class="forms mb">
            	<div class="formsList1 clearfix">
							<dl>
								<dt>货主姓名</dt>
					            <dd>
					            	 <input class="input160" name="shipperName" id="shipperName" value="$!{query.shipperName}"/>
					            </dd>
					        </dl> 
							<dl>
								<dt>交易类型</dt>
					            <dd>
					            	 <input class="input160" name="tradeType" id="tradeType" value="$!{query.tradeType}"/>
					            </dd>
					        </dl> 
							<dl>
								<dt>购买日期</dt>
					            <dd>
					            	 <input class="input160" name="buyDatetime" id="buyDatetime" value="$!{query.buyDatetime}"/>
					            </dd>
					        </dl> 
							<dl>
								<dt>车牌号</dt>
					            <dd>
					            	 <input class="input160" name="licenseplatenumber" id="licenseplatenumber" value="$!{query.licenseplatenumber}"/>
					            </dd>
					        </dl> 
                    <div class="clear"></div>
                    <div><input type="button" class="button_Green searchBtn " value="搜 索"/></div>
                </div>
            </div>
            <div class="createNew ">
            		<div class="fl mb mt"></div>
                <div class="fr mb"><input type="button" id="addButton" value="新 建" class="button_Green"/></div>
            </div>
        	<div class="tabBox queryCost1 mb">
            	<table width="100%" cellpadding="0" cellspacing="0">
            <tr>
						<th class="pl40"></th>
						<th>货主姓名</th>
						<th>交易类型</th>
						<th>购买日期</th>
						<th>车牌号</th>
						<th>单价</th>
						<th>吨位</th>
						<th>总金额</th>
						<th>矿名</th>
						<th>已付金额</th>
						<th>未付金额</th>
						<th>创建时间</th>
						<th>创建人</th>
                <th>操作</th>
            </tr>
             #if($!{pageInfos})
                #foreach($!{info} in $!{pageInfos})
	            	<tr>
								<td class="pl40">$!{info.shipperName}</td>
								<td>$!{info.tradeType}</td>
								<td>$!{info.buyDatetime}</td>
								<td>$!{info.licenseplatenumber}</td>
								<td>$!{info.unitprice}</td>
								<td>$!{info.onnage}</td>
								<td>$!{info.totalamount}</td>
								<td>$!{info.mineName}</td>
								<td>$!{info.paidAmount}</td>
								<td>$!{info.unpaidAmount}</td>
								<td>$!{info.createTime}</td>
								<td>$!{info.creator}</td>
		                <td>
			                <a href="../shipperbill/toEdit.do?id=$!{info.id}">编辑</a>
			                <a href="../shipperbill/shipperbillDelete.do?id=$!{info.id}">删除</a>
			                <a href="../shipperbill/shipperbillDetail.do?id=$!{info.id}">详情</a>
		                </td>
	           		</tr>
	             #end
	    	 #end
        </table>
			</div>
            <div class="fr">
            	<!--公用翻页代码-->
                #set($attr='formSubmit')
            	#showPageFormSubmit($pageInfos $attr)
                <!--END公用翻页代码-->
            </div>
            <div class="clear"></div>
            
        </div>
        </form>
    </div>
    <!--end mainContent-->
<script type="text/javascript">
$(document).ready(function(){
	$(".button_Green.searchBtn").bind("click",function(){
		$("#formSubmit").submit();
	});
	
	$("#addButton").bind("click",function(){
		location.href="../shipperbill/toAdd.do";
	});
});

</script>